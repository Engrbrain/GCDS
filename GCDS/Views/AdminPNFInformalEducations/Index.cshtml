@model IEnumerable<GCDS.Models.PNFInformalEducation>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AMLCompanyProfile.UserId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PNFPersonalDetails.Surname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NameOfTrainingCenter_Place)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NameOfTrainer)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AddressOfTrainer)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SpecialisedSkills_TrainingAcquired)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TimeStamp)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Is_Deleted)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateAcquired)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AMLCompanyProfile.UserId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PNFPersonalDetails.Surname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NameOfTrainingCenter_Place)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NameOfTrainer)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AddressOfTrainer)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SpecialisedSkills_TrainingAcquired)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TimeStamp)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Is_Deleted)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateAcquired)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
