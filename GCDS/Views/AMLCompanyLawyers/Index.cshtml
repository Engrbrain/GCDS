@model IEnumerable<GCDS.Models.AMLCompanyLawyer>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AMLCompanyProfile.UserId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NameOfLawyerOrLegalAdvisor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AddressOfLawyerOrLegalAdvisor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NameOfAccountant)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AddressOfAccountant)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NameOfConsultant)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AddressOfConsultant)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NameOfAuditor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TimeStamp)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Is_Deleted)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AddressOfAuditor)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AMLCompanyProfile.UserId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NameOfLawyerOrLegalAdvisor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AddressOfLawyerOrLegalAdvisor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NameOfAccountant)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AddressOfAccountant)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NameOfConsultant)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AddressOfConsultant)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NameOfAuditor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TimeStamp)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Is_Deleted)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AddressOfAuditor)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
