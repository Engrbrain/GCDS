                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()

                    <div class="form-horizontal">
                        <h4>PNFEmploymentHistory</h4>
                        <hr />
                        @Html.ValidationSummary(true, "", new { @class = "text-danger" })

                        @Html.HiddenFor(model => model.AMLCompanyProfileId)
                        @Html.HiddenFor(model => model.AMLCompanyProfile)
                        @Html.HiddenFor(model => model.Id)
                        @Html.HiddenFor(model => model.Is_Deleted)
                        @Html.HiddenFor(model => model.PNFPersonalDetails)
                        @Html.HiddenFor(model => model.PNFPersonalDetailsId)
                        @Html.HiddenFor(model => model.TimeStamp)

                        

                        <div class="form-group">
                            <span class="control-label col-md-6">Full name of Organisation</span>
                            <div class="col-md-10">
                                @Html.EditorFor(model => model.OrganisationFullName, new { htmlAttributes = new { @class = "form-control" } })
                                @Html.ValidationMessageFor(model => model.OrganisationFullName, "", new { @class = "text-danger" })
                            </div>
                        </div>

                        <div class="form-group">
                            <span class="control-label col-md-6">Have you engaged in apprenticeship before?</span>
                            <div class="col-md-10">
                                <div class="checkbox">
                                    @Html.EditorFor(model => model.Is_Apprenticeship)
                                    @Html.ValidationMessageFor(model => model.Is_Apprenticeship, "", new { @class = "text-danger" })
                                </div>
                            </div>
                        </div>

                        <div class="form-group">
                            <span class="control-label col-md-6">Have you been self-employed before?</span>
                            <div class="col-md-10">
                                <div class="checkbox">
                                    @Html.EditorFor(model => model.Is_SelfEmployment)
                                    @Html.ValidationMessageFor(model => model.Is_SelfEmployment, "", new { @class = "text-danger" })
                                </div>
                            </div>
                        </div>

                        <div class="form-group">
                            <span class="control-label col-md-6">Have you been unemployed before?</span>
                            <div class="col-md-10">
                                <div class="checkbox">
                                    @Html.EditorFor(model => model.Is_UnEmployment)
                                    @Html.ValidationMessageFor(model => model.Is_UnEmployment, "", new { @class = "text-danger" })
                                </div>
                            </div>
                        </div>

                        <div class="form-group">
                            <span class="control-label col-md-6">Have you done your National Service?</span>
                            <div class="col-md-10">
                                <div class="checkbox">
                                    @Html.EditorFor(model => model.Is_NationalService)
                                    @Html.ValidationMessageFor(model => model.Is_NationalService, "", new { @class = "text-danger" })
                                </div>
                            </div>
                        </div>

                        <div class="form-group">
                            <span class="control-label col-md-6">Address of Organisation</span>
                            <div class="col-md-10">
                                @Html.EditorFor(model => model.AddressOFOrganisation, new { htmlAttributes = new { @class = "form-control" } })
                                @Html.ValidationMessageFor(model => model.AddressOFOrganisation, "", new { @class = "text-danger" })
                            </div>
                        </div>

                        <div class="form-group">
