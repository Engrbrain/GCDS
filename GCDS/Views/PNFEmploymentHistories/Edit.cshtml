                            <span class="control-label col-md-6">Start Date of Employment</span>
                            <div class="col-md-10">
                                @Html.EditorFor(model => model.EmploymentStartDate, new { htmlAttributes = new { @class = "form-control", type = "date" } })
                                @Html.ValidationMessageFor(model => model.EmploymentStartDate, "", new { @class = "text-danger" })
                            </div>
                        </div>

                        <div class="form-group">
                            <span class="control-label col-md-6">End date of Employment</span>
                            <div class="col-md-10">
