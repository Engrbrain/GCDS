@model IEnumerable<GCDS.Models.Memo>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AMLCompanyProfile.UserId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MemoReference)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MemoTitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MemoDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MemoType)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AMLCompanyProfile.UserId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MemoReference)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MemoTitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MemoDescription)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MemoType)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
